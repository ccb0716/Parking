    /* font: 14px &quot;Segoe UI&quot;, &quot;Microsoft YaHei&quot;; */
    padding: 0px 10px;
    selection-background-color: #00a7b3;
}



LineEdit:focus,
TextEdit:focus,
PlainTextEdit:focus {
    border: none;
    
}

/* 禁用状态的颜色也需要相应调整 */
LineEdit:disabled, TextEdit:disabled,
PlainTextEdit:disabled {
    color: rgba(255, 255, 255, 0.5); /* 文本颜色调整为半透明白色 */
