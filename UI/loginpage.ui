    <property name="styleSheet">
     <string notr="true">CheckBox {
    color: #00c2e6;
    font: 14px 'Segoe UI', 'Microsoft YaHei', 'PingFang SC';
    spacing: 8px;
    min-width: 28px;
    min-height: 22px;
    outline: none;
    margin-left: 1px;
    background-color: #03063f;
}

CheckBox::indicator {
    width: 18px;
    height: 18px;
    border-radius: 5px;
    border: 1px solid rgba(255, 255, 255, 0.48);
    background-color: rgba(255, 255, 255, 0.022);
}

CheckBox::indicator:hover {
    border: 1px solid rgba(255, 255, 255, 0.56);
    background-color: rgba(255, 255, 255, 0.05);
}

CheckBox::indicator:pressed {
    border: 1px solid rgba(255, 255, 255, 0.27);
    background-color: rgba(255, 255, 255, 0.12);
}

/* 修改选中状态的背景颜色 */
CheckBox::indicator:checked,
CheckBox::indicator:indeterminate {
    border: 1px solid #009faa;
    background-color: #00c2e6; /* 更改为#00c2e6 */
}

CheckBox::indicator:checked:hover,
CheckBox::indicator:indeterminate:hover {
    border: 1px solid #00a7b3;
    background-color: #00a7b3;
}

CheckBox::indicator:checked:pressed,
CheckBox::indicator:indeterminate:pressed {
    border: 1px solid #3eabb3;
    background-color: #3eabb3;
}

CheckBox:disabled {
    color: rgba(255, 255, 255, 0.44);
}

CheckBox::indicator:disabled {
    border: 1px solid rgba(255, 255, 255, 0.27);
    background-color: transparent;
}

CheckBox::indicator:checked:disabled,
CheckBox::indicator:indeterminate:disabled {
    border: 1px solid rgb(128, 128, 128);
    background-color: rgb(128, 128, 128);
}</string>
